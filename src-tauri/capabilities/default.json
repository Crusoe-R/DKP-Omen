{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "window-state:default",
    "store:default",
    "dialog:default",
    {
    "identifier": "http:default",
    "allow": [{ "url": "https://raid-helper.dev/" }],
    "deny": [{ "url": "https://private.tauri.app" }]
    }
  ]
}
